<div class="movie">
    <h2>{{title}}</h2>
    <img
        src="{{imageUrl}}">
    <p>Available Tickets: {{tickets}}</p>
    <div class="btn-group">
        <button><a href="#/movie/editMovie/{{_id}}">Edit</a></button>
        <button><a href="#/movie/deleteMovie/{{_id}}">Delete</a></button>
        <button><a href="#/movie/buyTicket/{{_id}}">Buy Ticket</a></button>
        <button><a href="#/movie/detailsMovie/{{_id}}">Details</a></button>
    </div>
</div>